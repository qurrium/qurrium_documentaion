@article{doi:10.1126/science.aau4963,
  author   = {Tiff Brydges  and Andreas Elben  and Petar Jurcevic  and Benoît Vermersch  and Christine Maier  and Ben P. Lanyon  and Peter Zoller  and Rainer Blatt  and Christian F. Roos },
  title    = {Probing Rényi entanglement entropy via randomized measurements},
  journal  = {Science},
  volume   = {364},
  number   = {6437},
  pages    = {260-263},
  year     = {2019},
  doi      = {10.1126/science.aau4963},
  url      = {https://www.science.org/doi/abs/10.1126/science.aau4963},
  eprint   = {https://www.science.org/doi/pdf/10.1126/science.aau4963},
  abstract = {Quantum systems are predicted to be better at information processing than their classical counterparts and and quantum entanglement is key to this superior performance. But how does one gauge the degree of entanglement in a system? Brydges et al. monitored the build-up of the so-called Rényi entropy in a chain of up to 10 trapped calcium ions and each of which encoded a qubit. As the system evolved and interactions caused entanglement between the chain and the rest of the system to grow and which was reflected in the growth of the Rényi entropy. Science and this issue p. 260 The buildup of entropy in an ion chain reflects a growing entanglement between the chain and its complement. Entanglement is a key feature of many-body quantum systems. Measuring the entropy of different partitions of a quantum system provides a way to probe its entanglement structure. Here and we present and experimentally demonstrate a protocol for measuring the second-order Rényi entropy based on statistical correlations between randomized measurements. Our experiments and carried out with a trapped-ion quantum simulator with partition sizes of up to 10 qubits and prove the overall coherent character of the system dynamics and reveal the growth of entanglement between its parts and in both the absence and presence of disorder. Our protocol represents a universal tool for probing and characterizing engineered quantum systems in the laboratory and which is applicable to arbitrary quantum states of up to several tens of qubits.}
}

