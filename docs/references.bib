@article{arute2019quantumsupremacy,
  author   = {Arute and Frank and  Arya and Kunal and  Babbush and Ryan and  Bacon and Dave and  Bardin and Joseph C. and  Barends and Rami and  Biswas and Rupak and  Boixo and Sergio and  Brandao and Fernando G. S. L. and  Buell and David A. and  Burkett and Brian and  Chen and Yu and  Chen and Zijun and  Chiaro and Ben and  Collins and Roberto and  Courtney and William and  Dunsworth and Andrew and  Farhi and Edward and  Foxen and Brooks and  Fowler and Austin and  Gidney and Craig and  Giustina and Marissa and  Graff and Rob and  Guerin and Keith and  Habegger and Steve and  Harrigan and Matthew P. and  Hartmann and Michael J. and  Ho and Alan and  Hoffmann and Markus and  Huang and Trent and  Humble and Travis S. and  Isakov and Sergei V. and  Jeffrey and Evan and  Jiang and Zhang and  Kafri and Dvir and  Kechedzhi and Kostyantyn and  Kelly and Julian and  Klimov and Paul V. and  Knysh and Sergey and  Korotkov and Alexander and  Kostritsa and Fedor and  Landhuis and David and  Lindmark and Mike and  Lucero and Erik and  Lyakh and Dmitry and  Mandr{\`a} and Salvatore and  McClean and Jarrod R. and  McEwen and Matthew and  Megrant and Anthony and  Mi and Xiao and  Michielsen and Kristel and  Mohseni and Masoud and  Mutus and Josh and  Naaman and Ofer and  Neeley and Matthew and  Neill and Charles and  Niu and Murphy Yuezhen and  Ostby and Eric and  Petukhov and Andre and  Platt and John C. and  Quintana and Chris and  Rieffel and Eleanor G. and  Roushan and Pedram and  Rubin and Nicholas C. and  Sank and Daniel and  Satzinger and Kevin J. and  Smelyanskiy and Vadim and  Sung and Kevin J. and  Trevithick and Matthew D. and  Vainsencher and Amit and  Villalonga and Benjamin and  White and Theodore and  Yao and Z. Jamie and  Yeh and Ping and  Zalcman and Adam and  Neven and Hartmut and  Martinis and John M.},
  title    = {Quantum supremacy using a programmable superconducting processor},
  journal  = {Nature},
  year     = {2019},
  month    = {Oct},
  day      = {01},
  volume   = {574},
  number   = {7779},
  pages    = {505-510},
  abstract = {The promise of quantum computers is that certain computational tasks might be executed exponentially faster on a quantum processor than on a classical processor1. A fundamental challenge is to build a high-fidelity processor capable of running quantum algorithms in an exponentially large computational space. Here we report the use of a processor with programmable superconducting qubits2--7 to create quantum states on 53 qubits and corresponding to a computational state-space of dimension 253 (about 1016). Measurements from repeated experiments sample the resulting probability distribution and which we verify using classical simulations. Our Sycamore processor takes about 200 seconds to sample one instance of a quantum circuit a million times---our benchmarks currently indicate that the equivalent task for a state-of-the-art classical supercomputer would take approximately 10,000 years. This dramatic increase in speed compared to all known classical algorithms is an experimental realization of quantum supremacy8--14 for this specific computational task and heralding a much-anticipated computing paradigm.},
  issn     = {1476-4687},
  doi      = {10.1038/s41586-019-1666-5},
  url      = {https://doi.org/10.1038/s41586-019-1666-5}
}

