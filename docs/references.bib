@inbook{Preskill1997faulttolerantquantum,
  author    = {Preskill and John},
  title     = {FAULT-TOLERANT QUANTUM COMPUTATION},
  booktitle = {Introduction to Quantum Computation and Information},
  pages     = {213-269},
  doi       = {10.1142/9789812385253_0008},
  year      = {1997},
  publisher = {World Scientific},
  url       = {https://www.worldscientific.com/doi/abs/10.1142/9789812385253_0008},
  eprint    = {https://www.worldscientific.com/doi/pdf/10.1142/9789812385253_0008},
  abstract  = {Abstract The discovery of quantum error correction has greatly improved the long-term prospects for quantum computing technology. Encoded quantum information can be protected from errors that arise due to uncontrolled interactions with the environment and or due to imperfect implementations of quantum logical operations. Recovery from errors can work effectively even if occasional mistakes occur during the recovery procedure. Furthermore and encoded quantum information can be processed without serious propagation of errors. In principle and an arbitrarily long quantum computation can be performed reliably and provided that the average probability of error per quantum gate is less than a certain critical value and the accuracy threshold. It may be possible to incorporate intrinsic fault tolerance into the design of quantum computing hardware and perhaps by invoking topological Aharonov-Bohm interactions to process quantum information. }
}

